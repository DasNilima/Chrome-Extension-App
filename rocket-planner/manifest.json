{
    "manifest_version": 3,
    "name": "Rocket Planner",
    "version": "1.0",
    "description": "EPIC TASK PLANNER",
    "action": {
        "default_popup": "popup.html",
        "default_icons": {
            "16": "images/icons16.png",
            "32": "images/icons32.png",
            "48": "images/icons48.png",
            "128": "images/icons128.png"
        }
    },
    "permissions": [
        "storage",
        "tabs",
        "contextMenus"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "icons" :{
        "16": "images/icons16.png",
        "32": "images/icons32.png",
        "48": "images/icons48.png",
        "128": "images/icons128.png"
    }
}